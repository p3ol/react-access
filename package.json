{
  "name": "@poool/react-access",
  "version": "4.1.5",
  "description": "The easiest way to add Poool Access to your React app âœ¨",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p3ol/react-access.git"
  },
  "author": "Ugo Stephant <ugo@poool.fr>",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "workspaces": [
    "examples/*"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@junipero/core": "3.12.3",
    "@types/poool-access": "5.18.1"
  },
  "devDependencies": {
    "@poool/eslint-config": "5.0.7",
    "@poool/eslint-config-react": "5.0.9",
    "@swc/core": "1.14.0",
    "@swc/jest": "0.2.39",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/webpack": "5.28.5",
    "@types/webpack-dev-server": "4.7.2",
    "commit-and-tag-version": "12.6.0",
    "eslint": "9.38.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.0",
    "html-webpack-plugin": "5.6.4",
    "jest": "30.2.0",
    "jest-dev-server": "11.0.0",
    "jest-environment-jsdom": "30.2.0",
    "puppeteer": "24.26.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "7.9.4",
    "swc-loader": "0.2.6",
    "ts-node": "10.9.2",
    "tsdown": "0.15.9",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "webpack": "5.102.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "scripts": {
    "clean": "rm -rf ./dist || true",
    "build": "yarn clean && yarn build:code && yarn build:types",
    "build:code": "tsdown",
    "build:types": "tsc --project ./tsconfig.build.json",
    "prepack": "yarn build && yarn lint && yarn tsc && yarn test",
    "test": "NODE_ENV='test' jest",
    "lint": "eslint --max-warnings=0 --cache .",
    "release": "commit-and-tag-version && git push --follow-tags",
    "release:dry": "commit-and-tag-version --dry-run",
    "example:basic": "yarn workspace poool-react-access-basic-example serve",
    "example:next": "yarn workspace poool-react-access-next-example dev"
  },
  "resolutions": {
    "ws": "8.12.0"
  },
  "targets": [
    "chrome109",
    "edge138",
    "firefox140",
    "safari18",
    "opera80",
    "node20"
  ],
  "packageManager": "yarn@4.10.3"
}
